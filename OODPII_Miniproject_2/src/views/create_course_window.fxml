<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="310.0" prefWidth="358.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.CreateCourseController">
   <children>
      <GridPane layoutX="14.0" layoutY="10.0" prefHeight="202.0" prefWidth="313.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="55.0" minHeight="10.0" prefHeight="41.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="122.0" minHeight="0.0" prefHeight="42.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="161.0" minHeight="0.0" prefHeight="28.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="161.0" minHeight="6.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="161.0" minHeight="0.0" prefHeight="28.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Course Name:" />
            <Label text="Teacher:" GridPane.rowIndex="2" />
            <Label text="Course ID:" GridPane.rowIndex="1" />
            <TextField fx:id="courseNameText" GridPane.columnIndex="2" />
            <TextField fx:id="courseIDText" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Label text="First Name:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Last Name:" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="firstName" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <TextField fx:id="lastName" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <Label text="Signature:" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="signature" GridPane.columnIndex="2" GridPane.rowIndex="4" />
         </children>
      </GridPane>
      <ButtonBar layoutX="127.0" layoutY="246.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button fx:id="okButton" mnemonicParsing="false" onAction="#handleOk" text="OK" />
          <Button mnemonicParsing="false" onAction="#cancelButton" text="Cancel" />
        </buttons>
      </ButtonBar>
      <CheckBox fx:id="isExaminator" layoutX="227.0" layoutY="212.0" mnemonicParsing="false" text="Examinator" />
   </children>
</AnchorPane>
