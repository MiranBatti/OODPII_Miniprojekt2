<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.MenuController">
   <children>
      <VBox layoutX="-1.0" prefHeight="342.0" prefWidth="311.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0" />
            <HBox alignment="CENTER" prefHeight="67.0" prefWidth="324.0">
               <children>
                  <Button fx:id="createButton" mnemonicParsing="false" onAction="#createButtonClick" prefHeight="43.0" prefWidth="123.0" text="Create Course" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="78.0" prefWidth="324.0">
               <children>
                  <Button fx:id="viewCoursesButton" mnemonicParsing="false" onAction="#viewClick" prefHeight="43.0" prefWidth="120.0" text="View Courses" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
